{
    "headers": [
      {
        "source": "/(.*)\\.html",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/html; charset=UTF-8"
          }
        ]
      },
      {
        "source": "/(.*)\\.css",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/css; charset=UTF-8"
          }
        ]
      },
      {
        "source": "/(.*)\\.js",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript; charset=UTF-8"
          }
        ]
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/programs",
        "dest": "/programs.html"
      },
      {
        "src": "/articles",
        "dest": "/articles.html"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    "cleanUrls": true
  }